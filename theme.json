{
  "name": "RustPress Enterprise (Static)",
  "version": "2.0.0-static",
  "description": "Static HTML version of the RustPress Enterprise theme. A stunning, animated landing page that works without any backend. Features rust-orange color scheme, modern animations, and a complete static site structure.",
  "author": "RustPress Team",
  "license": "MIT",
  "homepage": "https://rustpress.net",
  "repository": "https://github.com/rustpress-net/rustpress-theme-rustpress-enterprise",
  "keywords": [
    "static-site",
    "html",
    "theme",
    "landing-page",
    "animated",
    "no-backend"
  ],
  "type": "static",
  "main": "index.html",
  "pages": {
    "home": "index.html",
    "themes": "themes.html",
    "plugins": "plugins.html",
    "features": "features.html",
    "pricing": "pricing.html",
    "about": "about.html",
    "ide": "ide.html",
    "roadmap": "roadmap.html",
    "contact": "contact.html"
  },
  "assets": {
    "css": [
      "assets/css/variables.css",
      "assets/css/base.css",
      "assets/css/typography.css",
      "assets/css/layout.css",
      "assets/css/components.css",
      "assets/css/animations.css",
      "assets/css/sections.css",
      "assets/css/responsive.css"
    ],
    "js": [
      "assets/js/animations.js",
      "assets/js/navigation.js",
      "assets/js/particles.js",
      "assets/js/counters.js",
      "assets/js/forms.js",
      "assets/js/gallery.js",
      "assets/js/main.js"
    ]
  },
  "colors": {
    "primary": "#CE422B",
    "secondary": "#B87333",
    "accent": "#E97451",
    "background": "#0D0D0D",
    "surface": "#1A1A1A",
    "text": "#FFFFFF",
    "text-muted": "#A0A0A0"
  },
  "fonts": {
    "heading": "Space Grotesk",
    "body": "Inter",
    "code": "JetBrains Mono"
  }
}
